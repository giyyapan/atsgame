Class.sub = function (){
	var parent = this;
	var c = function (){
		parent.call(this);
		if(c.prototype._init){
			c.prototype._init.apply(this,arguments);
		}
	}
	c.parent = this;
	c.prototype = new c.parent();
	c.prototype.parent = c.parent;
	c.prototype._class = c;
	c.sub = Class.sub;
	c.extend = Class.extend;
	return c;
}
